scalar Date

type Query {
  bookById(id: ID): Book
  authorById(id:ID): [Author]
  userById(id:ID): User
}

type Book {
  id: ID!
  name: String
  pageCount: Int
  author: Author
}

type Author {
  id: ID
  firstName: String
  lastName: String
}

type User {
  id: ID
  name: String
  pwd: String
  date: Date
}

